//test
var mg = require("mongodb").MongoClient;
mg.connect('mongodb://localhost:27017/fcc-bep3',function(er,db){
        if(er){
            throw er;
        }
        var urlL = db.collection('UrlList');
        var repu = urlL.find({url:'eee'});
        console.log('repu record',repu.count());
        db.close();
})